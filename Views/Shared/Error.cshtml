@model Studentdata.ErorrModels.ErrorViewModel

<h1 class="text-danger">Oops! Something went wrong.</h1>

<p>@Model.ErrorMessage</p>

@if (Model.ShowRequestId)
{
    <p><strong>Request ID:</strong> @Model.RequestId</p>
}

<a asp-action="Index" asp-controller="Student" class="btn btn-primary">Back to Home</a>
